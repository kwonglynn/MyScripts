MOLINFO STRUCTURE=protein.pdb
WHOLEMOLECULES ENTITY0=1-319
hh: ALPHARMSD RESIDUES=all TYPE=DRMSD R_0=0.08

prot: GROUP NDX_FILE=index.ndx NDX_GROUP=Protein
P_up: GROUP NDX_FILE=index.ndx NDX_GROUP=P_up

prot_P: COORDINATION GROUPA=prot GROUPB=P_up R_0=0.3 D_0=0.0 NN=6 MM=10 NLIST NL_CUTOFF=0.8 NL_STRIDE=500

prot_COM: COM ATOMS=prot
P_up_COM: COM ATOMS=P_up
dist: DISTANCE ATOMS=prot_COM,P_up_COM

PRINT ARG=* STRIDE=100 FILE=COLVAR FMT=%10.3f
